package myWhile;

public class NoteSlide7 {

	public static void main(String[] args) {

		int i=1;
		int j=10;
		int k=20;
		
		do {
			System.out.print(i + " ");
			i++;
		}while(i<5);
		
		do {
			System.out.print(j + " ");
			j++;
		}while(j<20);
		
		do {
			System.out.print(k + " ");
			k--;
		}while(k>10);
	}

}
