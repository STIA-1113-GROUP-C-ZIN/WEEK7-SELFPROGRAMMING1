package myWhile;

public class NoteSlide8 {

	public static void main(String[] args) {

		int i;
		int j;
		int k;
		int l;
		
		for(i=1; i<5; i++) {
		System.out.print(i + " ");
		}
		System.out.println();
		
		for(j=10; j<20; j++) {
		System.out.print(j + " ");
		}
		System.out.println();

		for(k=20; k>10; k--) {
		System.out.print(k + " ");
		}
		System.out.println();

		for(l=30; l<40; l++) {
		System.out.print(l + " ");
		}
		System.out.println();

	}

}
